def factorial(Array[1, int] n) -> Array[1, int]{

    def one() -> Array[1, int]{
        return [1]
    }

    print(n[0])

    if (n[0] == 0 or n[0] == 1){
        return one()[0]
    } else {
        return n[0] * factorial(n-1)[0]
    }
}

Array[1, int] dane = [5]

Array[1, int] wynik = factorial(dane)

print(wynik)