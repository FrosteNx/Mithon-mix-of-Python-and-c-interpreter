def factorial(int n) -> int{

    def one() -> int{
        def one() -> int{
            return 1
        }
    }

    def two() -> int{
        return 2
    }

    if (n == 0 or n == 1){
        return one()
    } else {
        return n * factorial(n-1)
    }
}

def helper(int a, int b) -> int{
    return a + b
}


